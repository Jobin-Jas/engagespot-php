{
    "name": "jobin/engagespot",
    "type": "library",
    "description": "engagespot php package",
    "keywords": [
        "jobin",
        "engagespot"
    ],
    "homepage": "https://github.com/Jobin-Jas/engagespot-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jobin Jose",
            "email": "jobin8157@gmail.com",
            "homepage": "https://github.com/Jobin-Jas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "jobin\\engagespot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jobin\\engagespot\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
